{
    "name": "<%= project %>",
    "version": "1.0.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/tenorok/<%= project %>.git"
    },
    "engines": {
        "node": "<%= nodejs %>",
        "npm": "<%= npm %>"<% if (mongodb !== 'no') { -%>,
        "mongodb": "<%= mongodb %>"<% } %>
    },
    "scripts": {
        "prettier": "prettier --write .",
        "task": "node ./tasksfile.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "author": {
        "name": "Artem Kurbatov",
        "email": "mail@tenorok.ru",
        "homepage": "https://tenorok.ru"
    },
<% if (monorepo) { -%>
    "monorepo": {
        "name": "@monorepo",
        "dependencies": {
            "package": "../package/app/dist/",
        }
    },
<% } -%>
<% if (mongodb !== 'no') { -%>
    "config": {
        "migration": 0
    },
<% } -%>
    "dependencies": {
    },
    "devDependencies": {
    }
}
